{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JWTOptions": {
    "SecretKey": "my-dub-auth-secret-key-my-dub-auth-secret-key",
